{
  "name": "rnew",
  "version": "0.0.3",
  "private": true,
  "author": "Nestoro",
  "license": "MIT",
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "test:web": "react-scripts test --env=jsdom",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ios": "react-native run-ios",
    "ios:release": "react-native bundle --platform=ios",
    "preandroid": "scripts/preandroid.sh",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:dev_menu": "adb shell input keyevent KEYCODE_MENU",
    "android:emulator": "scripts/android:emulator.js",
    "android:kill_server": "fuser -k 8081/tcp || true",
    "android:log": "react-native log-android",
    "android:release": "cd android && ./gradlew assembleRelease",
    "preweb:release": "npm run icon-gen",
    "preweb:release:serve": "npm run web:release",
    "web": "react-scripts start",
    "web:release": "react-scripts build",
    "web:release:serve": "serve -s build",
    "preelectron": "scripts/preelectron.sh",
    "preelectron:release": "npm run preelectron",
    "electron": "electron build",
    "electron:release": "scripts/electron:release.sh",
    "release": "scripts/release.sh",
    "eject": "react-scripts eject",
    "icon-gen": "scripts/icon-gen.sh",
    "install": "node scripts/install.js",
    "clear": "rm -fr $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-*"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-art": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-native": "^0.63.0",
    "react-native-web": "^0.14.13",
    "react-router-dom": "^5.1.2",
    "react-router-native": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime": "^7.6.3",
    "@types/babel__core": "^7.1.3",
    "@types/jest": "^24.0.20",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-router-native": "^5.1.0",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-native-web": "^0.10.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "electron": "^11.2.3",
    "electron-packager": "^13.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "icon-gen": "^2.1.0",
    "jest": "26.6.0",
    "metro-react-native-babel-preset": "^0.56.3",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.2",
    "react-test-renderer": "16.9.0",
    "rnpm-plugin-dom": "^0.5.0",
    "rnpm-plugin-windows": "^0.6.1",
    "serve": "^11.3.2",
    "typescript": "^3.6.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)x?$",
    "globals": {
      "window": {}
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-router-native)/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.*",
      "src/utilities/routing/index.*"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
